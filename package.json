{
  "name": "ignite-design-system",
  "version": "0.1.2",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ignite-design-system -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "ci:test": "npm run test:ci",
    "ci:package": "npm run build"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.ts"
  },
  "repository": {
    "type": "git",
    "url": "http://devgit.dev.us.corp:7990/projects/NUI/repos/ignite-design-system"
  },
  "publishConfig": {
    "registry": "http://artifactory.mia.ucloud.int/artifactory/api/npm/npm-local"
  },
  "author": {
    "name": "Derek Stobbe",
    "email": "derek_stobbe@ultimatesoftware.com"
  },
